ls/Meals.js\";\nimport CartProvider from \"./store/CartProvider.js\";\nfunction App() {\n  const [cartIsVisible,setCartIsVisible]=useState(false);\n  const toggleCarthandler=()=>{\n    setCartIsVisible((prv)=>{\nreturn !prv;\n    })\n  }\n  return (\n    <CartProvider>\n      {cartIsVisible&&<Cart onCartClose={toggleCarthandler}/>}\n      <Header onCartClick={toggleCarthandler}/>\n      <main>\n        <Meals></Meals>\n      </main>\n    </CartProvider>\n  );\n}\n\nexport default App;\n","C:\\javascript and reactjs\\Food Order App\\src\\components\\Layout\\Header.js",[],"C:\\javascript and reactjs\\Food Order App\\src\\components\\Layout\\HeaderCartButton.js",["86"],"import react, { useContext, useEffect,useState } from \"react\";\r\nimport styles from \"./HeaderCartButton.module.css\";\r\nimport CartIcon from \"../Cart/CartIcon\";\r\nimport CartContext from \"../../store/cart-context\";\r\nconst HeaderCartButton = (props) => {\r\n  const [buttonIsBump, setButtonIsBump] = useState(false);\r\n  const ctx = useContext(CartContext);\r\n  const items=ctx.items;\r\n  const numberofCartitems = items.reduce((curr, item) => {\r\n    return curr + item.amount;\r\n  }, 0);\r\n  const buttonclasses = `${styles.button} ${buttonIsBump?styles.bump : ''}`;\r\n  useEffect(() => {\r\n    if (items.length === 0) {\r\n      return;\r\n    }\r\n    setButtonIsBump(true);\r\n    const timer = setTimeout(() => {\r\n      setButtonIsBump(false);\r\n    }, 300);\r\n    return () => {\r\n      clearTimeout(timer);\r\n    };\r\n  }, [items]);\r\n  return (\r\n    <button className={buttonclasses} onClick={props.onClick}>\r\n      <span className={styles.icon}>\r\n        <CartIcon />\r\n      </span>\r\n      <span>Your Cart</span>\r\n      <span className={styles.badge}>{numberofCartitems}</span>\r\n    </button>\r\n  );\r\n};\r\n\r\nexport default HeaderCartButton;\r\n","C:\\javascript and reactjs\\Food Order App\\src\\components\\Cart\\CartIcon.js",[],"C:\\javascript and reactjs\\Food Ord